{% assign enabled = true %}
{% if enabled %}
    {% unless settings.slides_mode %}
    {%- if section.blocks.size > 0 -%}
        <div class="holder  {% if section.settings.block_width == "fullboxed" %} fullwidth{% else %} {{ section.settings.block_width }}{% endif %} {{ section.settings.outer_margin }} full-nopad {% if section.settings.show_under_header %}show_under_header{% endif %}">
            <div class="container">
                {% if section.settings.right_column %}
                <div class="row row--sm-pad {% if section.settings.column == 'left' %}flex-md-row-reverse{% endif %}">
                    <div class="col-md-9 centerColumn">
                        {% endif %}
                        <div class="bnslider-wrapper">
                            <div class="bnslider bnslider--{{ section.settings.slideshow_height }} {% if section.settings.arowsdark %}bnslider--darkarrows{% endif %} {% if section.settings.keep_scale %}keep-scale{% endif %}" {% if section.settings.keep_scale_width %}data-start-width='{{ section.settings.keep_scale_width }}'{% endif %}  {% if section.settings.keep_scale_height %}data-start-height='{{ section.settings.keep_scale_height }}'{% endif %} {% if section.settings.keep_scale_mwidth %}data-start-mwidth='{{ section.settings.keep_scale_mwidth }}'{% endif %}  {% if section.settings.keep_scale_mheight %}data-start-mheight='{{ section.settings.keep_scale_mheight }}'{% endif %} id="bnslider-{{ section.id }}" data-slick='{"dots": {{ section.settings.dots }}}' data-autoplay="{{ section.settings.autoplay }}" data-speed="{{ section.settings.autoplay_speed }}">
                                {% for block in section.blocks %}
                                    {% if block.settings.enable and block.type == 'image' %}
                                        {%- assign is_background_video = false -%}
                                        {% if block.settings.type == 'box' or block.settings.type == 'background-box' or  block.settings.type == 'background' %}
                                            {% unless block.settings.link_all_slide %}
                                        <div class="bnslider-slide {% if block.settings.type == 'background-box' or  block.settings.type == 'background' %}bnslider-slide--background-video{% endif %}">
                                        {% else %}
                                            <a href="{% if block.settings.link != blank %}{{ block.settings.link }}{% else %}#{% endif %}" target="{{ block.settings.link_target }}" class="bnslider-slide {% if block.settings.type == 'background-box' or  block.settings.type == 'background' %}bnslider-slide--background-video{% endif %}">
                                                {% endunless %}
                                                {% unless block.settings.type == 'background' %}
                                                <div class="video-loader"><i class="spinner icon-spinner3"></i></div>
                                                <button type="button" class="bnslider-video-control bnslider-video-control--close" data-controls="bnsliderVideo-{{ block.id }}">
                                                    <svg width="80" height="80" class="slider-control-icon" viewBox="0 0 100 100">
                                                        <rect x="4" y="4" width="92" height="92" class="box"></rect>
                                                        <line x1="35" y1="35" x2="65" y2="65" stroke="#fff" stroke-width="4.5" stroke-linecap="round" stroke-miterlimit="10"></line>
                                                        <line x1="65" y1="35" x2="35" y2="65" stroke="#fff" stroke-width="4.5" stroke-linecap="round" stroke-miterlimit="10"></line>
                                                    </svg>
                                                </button>
                                                <button type="button" class="bnslider-video-control bnslider-video-control--play" data-controls="bnsliderVideo-{{ block.id }}" data-tooltip="Youtube Player">
                                                    <svg width="80" height="80" class="slider-control-icon" viewBox="0 0 100 100">
                                                        <rect x="4" y="4" width="92" height="92" class="box"></rect>
                                                        <polygon points="30,30 70,50 30,70" class="play"/>
                                                    </svg>
                                                </button>
                                                {% endunless %}
                                                <div id="bnsliderVideo-{{ block.id }}" class="bnslider-video {% if block.settings.type == 'background-box' or  block.settings.type == 'background' %} bnslider-video--background {% endif %} {% if block.settings.type == 'box' %} bnslider-video--box {% endif %} " data-id="{{ block.settings.video_id }}" data-type="{{ block.settings.type }}" data-bnslider="bnslider-{{ section.id }}"></div>
                                                {% capture current %}{% cycle 1,2 %}{% endcapture %}
                                                {% if block.settings.image_mobile != blank %}
                                                    <div class="bnslider-image-mobile lazyload blur-up" {% if block.settings.image_mobile != blank %}data-bgset="{% include 'bgset', image: block.settings.image_mobile %}" data-sizes="auto"{% endif %}></div>
                                                {% else %}
                                                    <div class="bnslider-image-mobile">{% include 'image-placeholder-bg', text: '800x800' %}</div>
                                                {% endif %}
                                                {% if block.settings.image != blank %}
                                                    <div class="bnslider-image {{ block.settings.bg_v_alignment }} lazyload blur-up" {% if block.settings.image != blank %}data-bgset="{% include 'bgset', image: block.settings.image %}" data-sizes="auto"{% endif %}></div>
                                                {% else %}
                                                    <div class="bnslider-image">{% include 'image-placeholder-bg', text: '1920x800' %}</div>
                                                {% endif %}
                                                <div class="bnslider-text-wrap bnslider-overlay">
                                                    <div class="bnslider-text-content {{ block.settings.captions_v_alignment }} {{ block.settings.captions_h_alignment }}">
                                                        <div class="bnslider-text-content-flex {% if section.settings.block_width == 'fullboxed' %} container {% endif %}">
                                                            <div class="bnslider-vert {{ block.settings.captions_border }} {% if block.settings.captions_half %}w-50{% endif %}" data-animation="fadeIn" data-animation-delay="0.5s">

                                                                {% if block.settings.caption_num_yes %}
                                                                    <div class="bnslider-text bnslider-text--num-wrap text-{{ block.settings.caption_num_hor_alignment }} {% if block.settings.caption_num_hidemobile_yes %} d-none d-md-flex{% endif %}">
                                                                        <div class="bnslider-text--num" data-animation="{{ block.settings.caption_num1_animation }}" data-animation-delay="{{ block.settings.caption_num1_delay }}ms" data-fontcolor="{{ block.settings.caption_num1_color }}" data-fontweight="{{ block.settings.caption_num1_fontweight }}" data-otherstyle="{{ block.settings.caption_num1_othercss }}">{{ block.settings.caption_num1 }}</div>
                                                                        <div class="bnslider-text--num-text mt-0 bnslider-text--{{ block.settings.caption_num2_fontsize }}" data-animation="{{ block.settings.caption_num2_animation }}" data-animation-delay="{{ block.settings.caption_num2_delay }}ms" data-fontcolor="{{ block.settings.caption_num2_color }}" data-fontweight="{{ block.settings.caption_num2_fontweight }}" data-lineheight="{{ block.settings.caption_num2_lineheight }}" data-otherstyle="{{ block.settings.caption_num2_othercss }}">{{ block.settings.caption_num2 }}</div>
                                                                    </div>
                                                                {% endif %}

                                                                {%- for i in (1..3) -%}
                                                                    {%- capture enable -%}caption_{{ i }}_yes{%- endcapture -%}
                                                                    {%- capture hidemobile -%}caption_{{ i }}_hidemobile_yes{%- endcapture -%}
                                                                    {%- capture text -%}caption_{{ i }}{%- endcapture -%}
                                                                    {%- capture text_hor_alignment -%}caption_{{ i }}_hor_alignment{%- endcapture -%}
                                                                    {%- capture animation -%}caption_{{ i }}_animation{%- endcapture -%}
                                                                    {%- capture delay -%}caption_{{ i }}_delay{%- endcapture -%}
                                                                    {%- capture color -%}caption_{{ i }}_color{%- endcapture -%}
                                                                    {%- capture bgcolor -%}caption_{{ i }}_bgcolor{%- endcapture -%}
                                                                    {%- capture fontweight -%}caption_{{ i }}_fontweight{%- endcapture -%}
                                                                    {%- capture fontsize -%}caption_{{ i }}_fontsize{%- endcapture -%}
                                                                    {%- capture lineheight -%}caption_{{ i }}_lineheight{%- endcapture -%}
                                                                    {%- capture othercss -%}caption_{{ i }}_othercss{%- endcapture -%}
                                                                    {%- if block.settings[enable] %}
                                                                        <div class="bnslider-text bnslider-text--{{ block.settings[fontsize] }}  text-{{ block.settings[text_hor_alignment] }} {%- if block.settings[hidemobile] %} d-none d-md-block{%- endif -%}" data-animation="{{ block.settings[animation] }}" data-animation-delay="{{ block.settings[delay] }}ms" data-fontcolor="{{ block.settings[color] }}" data-bgcolor="{{ block.settings[bgcolor] }}" data-fontweight="{{ block.settings[fontweight] }}" data-lineheight="{{ block.settings[lineheight] }}" data-otherstyle="{{ block.settings[othercss] }}">{{ block.settings[text] }}</div>
                                                                    {%- endif -%}
                                                                {% endfor %}
                                                                {% if block.settings.link_button_yes %}

                                                                <div class="btn-wrap text-{{ block.settings.btn_hor_aligment }}"
                                                                     data-animation="{{ block.settings.button_animation }}"
                                                                     data-animation-delay="{{ block.settings.button_delay }}ms"
                                                                     data-otherstyle="{{ block.settings.button_othercss }}">
                                                                    {% if block.settings.link_button_small %}<span>{% endif %}
                                                                        {% unless block.settings.link_all_slide %}

                                                                        <a href="{% if block.settings.link != blank %}{{ block.settings.link }}{% else %}#{% endif %}" target="{{ block.settings.link_target }}" class="btn-decor"
                                                                           data-fontcolor="{{ block.settings.button_text_color }}">
                                                                                  {{ block.settings.link_button_text }}
                                                                            <span class="btn-line" data-bgcolor="{{ block.settings.button_bgcolor }}"></span>
                                                                        </a>

                                                                        {% else %}
                                                                        <div class="btn-decor"
                                                                             data-fontcolor="{{ block.settings.button_text_color }}">
                                                                                  {{ block.settings.link_button_text }}
                                                                            <span class="btn-line" data-bgcolor="{{ block.settings.button_bgcolor }}"></span>
                                                                              </div>
                                                                        {% endunless %}
                                                                        {% if block.settings.link_button_small %}</span>{% endif %}
                                                                    </div>

                                                                {% endif %}
                                                            </div>
                                                        </div>
                                                    </div>
                                                </div>
                                                {% unless block.settings.link_all_slide %}
                                            </div>
                                        {% else %}
                                            </a>
                                            {% endunless %}
                                        {% else %}
                                            {% unless block.settings.link_all_slide %}
                                            <div class="bnslider-slide">
                                        {% else %}
                                            <a href="{% if block.settings.link != blank %}{{ block.settings.link }}{% else %}#{% endif %}" target="{{ block.settings.link_target }}" class="bnslider-slide">
                                                {% endunless %}
                                                {% capture current %}{% cycle 1,2 %}{% endcapture %}
                                                {% if block.settings.image_mobile != blank %}
                                                    <div class="bnslider-image-mobile lazyload blur-up" {% if block.settings.image_mobile != blank %}data-bgset="{% include 'bgset', image: block.settings.image_mobile %}" data-sizes="auto"{% endif %}></div>
                                                {% else %}
                                                <div class="bnslider-image-mobile">{% include 'image-placeholder-bg', text: '800x800' %}</div>
                                                {% endif %}
                                                {% if block.settings.image != blank %}
                                                <div class="bnslider-image {{ block.settings.bg_v_alignment }} lazyload blur-up" {% if block.settings.image != blank %}data-bgset="{% include 'bgset', image: block.settings.image %}" data-sizes="auto"{% endif %}></div>
                                                {% else %}
                                                    <div class="bnslider-image">{% include 'image-placeholder-bg', text: '1920x800' %}</div>
                                                {% endif %}
                                                <div class="bnslider-text-wrap bnslider-overlay">
                                                    <div class="bnslider-text-content {{ block.settings.captions_v_alignment }} {{ block.settings.captions_h_alignment }}">
                                                        <div class="bnslider-text-content-flex {% if section.settings.block_width == 'fullboxed' %} container {% endif %}">
                                                            <div class="bnslider-vert {{ block.settings.captions_border }} {% if block.settings.captions_half %}w-50 {% unless block.settings.caption_num_yes %}mx-0{% endunless %}{% endif %}" data-animation="fadeIn" data-animation-delay="0.5s">

                                                                {% if block.settings.caption_num_yes %}
                                                                    <div class="bnslider-text bnslider-text--num-wrap text-{{ block.settings.caption_num_hor_alignment }} {% if block.settings.caption_num_hidemobile_yes %} d-none d-md-flex{% endif %}">
                                                                        <div class="bnslider-text--num" data-animation="{{ block.settings.caption_num1_animation }}" data-animation-delay="{{ block.settings.caption_num1_delay }}ms" data-fontcolor="{{ block.settings.caption_num1_color }}" data-fontweight="{{ block.settings.caption_num1_fontweight }}" data-otherstyle="{{ block.settings.caption_num1_othercss }}">{{ block.settings.caption_num1 }}</div>
                                                                        <div class="bnslider-text--num-text mt-0 bnslider-text--{{ block.settings.caption_num2_fontsize }}" data-animation="{{ block.settings.caption_num2_animation }}" data-animation-delay="{{ block.settings.caption_num2_delay }}ms" data-fontcolor="{{ block.settings.caption_num2_color }}" data-fontweight="{{ block.settings.caption_num2_fontweight }}" data-lineheight="{{ block.settings.caption_num2_lineheight }}" data-otherstyle="{{ block.settings.caption_num2_othercss }}">{{ block.settings.caption_num2 }}</div>
                                                                    </div>
                                                                {% endif %}

                                                                {%- for i in (1..3) -%}
                                                                    {%- capture enable -%}caption_{{ i }}_yes{%- endcapture -%}
                                                                    {%- capture hidemobile -%}caption_{{ i }}_hidemobile_yes{%- endcapture -%}
                                                                    {%- capture text -%}caption_{{ i }}{%- endcapture -%}
                                                                    {%- capture text_hor_alignment -%}caption_{{ i }}_hor_alignment{%- endcapture -%}
                                                                    {%- capture animation -%}caption_{{ i }}_animation{%- endcapture -%}
                                                                    {%- capture delay -%}caption_{{ i }}_delay{%- endcapture -%}
                                                                    {%- capture color -%}caption_{{ i }}_color{%- endcapture -%}
                                                                    {%- capture bgcolor -%}caption_{{ i }}_bgcolor{%- endcapture -%}
                                                                    {%- capture fontweight -%}caption_{{ i }}_fontweight{%- endcapture -%}
                                                                    {%- capture fontsize -%}caption_{{ i }}_fontsize{%- endcapture -%}
                                                                    {%- capture lineheight -%}caption_{{ i }}_lineheight{%- endcapture -%}
                                                                    {%- capture othercss -%}caption_{{ i }}_othercss{%- endcapture -%}
                                                                    {%- if block.settings[enable] %}
                                                                        <div class="bnslider-text bnslider-text--{{ block.settings[fontsize] }}  text-{{ block.settings[text_hor_alignment] }} {%- if block.settings[hidemobile] %} d-none d-md-block{%- endif -%}" data-animation="{{ block.settings[animation] }}" data-animation-delay="{{ block.settings[delay] }}ms" data-fontcolor="{{ block.settings[color] }}" data-bgcolor="{{ block.settings[bgcolor] }}" data-fontweight="{{ block.settings[fontweight] }}" data-lineheight="{{ block.settings[lineheight] }}" data-otherstyle="{{ block.settings[othercss] }}">{{ block.settings[text] }}</div>
                                                                    {%- endif -%}
                                                                {% endfor %}
                                                                {% if block.settings.link_button_yes %}


                                                                <div class="btn-wrap text-{{ block.settings.btn_hor_aligment }}"
                                                                     data-animation="{{ block.settings.button_animation }}"
                                                                     data-animation-delay="{{ block.settings.button_delay }}ms"
                                                                     data-otherstyle="{{ block.settings.button_othercss }}">
                                                                    {% if block.settings.link_button_small %}<span>{% endif %}
                                                                        {% unless block.settings.link_all_slide %}
                                                                        <a href="{% if block.settings.link != blank %}{{ block.settings.link }}{% else %}#{% endif %}" target="{{ block.settings.link_target }}" class="btn-decor"
                                                                           data-fontcolor="{{ block.settings.button_text_color }}">
                                                                                {{ block.settings.link_button_text }}
                                                                            <span class="btn-line" data-bgcolor="{{ block.settings.button_bgcolor }}"></span>
                                                                            </a>
                                                                        {% else %}
                                                                        <div class="btn-decor"
                                                                             data-fontcolor="{{ block.settings.button_text_color }}">
                                                                                {{ block.settings.link_button_text }}
                                                                            <span class="btn-line" data-bgcolor="{{ block.settings.button_bgcolor }}"></span>
                                                                            </div>
                                                                        {% endunless %}
                                                                        {% if block.settings.link_button_small %}</span>{% endif %}
                                                                    </div>

                                                                {% endif %}
                                                            </div>
                                                        </div>
                                                    </div>
                                                </div>
                                                {% unless block.settings.link_all_slide %}
                                            </div>
                                        {% else %}
                                            </a>
                                            {% endunless %}
                                        {% endif %}
                                    {% endif %}
                                {%- endfor %}
                            </div>
                            {% comment %}
                            <div class="bnslider-loader">
                                <div class="loader-wrap">
                                    <div class="dots">
                                        <div class="dot one"></div>
                                        <div class="dot two"></div>
                                        <div class="dot three"></div>
                                    </div>
                                </div>
                            </div>
                            {% endcomment %}
                            {% if section.settings.arrows %}
                                <div class="bnslider-arrows {% if section.settings.arowsbg %}bnslider-arrows--bg{% endif %} {% if section.settings.arowsshift %}{{ section.settings.arowsshiftside }}{% endif %} {% if section.settings.block_width == 'fullwidth' %}container-fluid{% else %}container{% endif %}">
                                    <div></div></div>{% endif %}
                            {% if section.settings.dots %}
                                <div class="bnslider-dots {% if section.settings.dotspos %}vert-dots {{ section.settings.dotsposide }}{% endif %} {% if section.settings.block_width == 'fullwidth' or section.settings.dotsfullwidth %}container-fluid{% else %}container{% endif %}"></div>
                            {% endif %}
                            {% if section.settings.snow %}
                                <canvas class="snow-effect" id="SnowEffect{{ section.id }}"></canvas>
                            {% endif %}
                        </div>
                        {% if section.settings.right_column %}
                    </div>
                    <div class="col-md-3 mt-15 mt-md-0 sideColumn">
                        {% assign gutters = 0.3 %}
                        {% assign body_w = 1170 %}
                        <div class="bnrs-column">
                            {% for block in section.blocks %}
                                {% if block.settings.enable and block.type == 'right_banner' %}
                                    {% if section.settings.bnr_source == 'admin' %}
                                        {% assign banner = block.settings %}
                                        {% assign col_md = 3 %}
                                        {% unless banner.image %}
                                        {% include 'image-placeholder-aspect-ratio', width: 530, height: 370 %}
                                        {%- endunless -%}
                                        {% render 'banner', banner: banner, col_md: col_md, gutters: gutters, body_w: body_w %}
                                    {% else %}
                                        {{ section.settings.html }}
                                    {% endif %}
                                {% endif %}
                            {% endfor %}
                        </div>
                    </div>
                </div>
                {% endif %}
            </div>
        </div>
    {% endif %}
    {% endunless %}
{% endif %}
{% schema %}
{
"name": "Slideshow",
"class": "index-section index-section--flush",
"max_blocks": 8,
"settings": [
{
"type": "paragraph",
"content": "[Documentation](http://big-skins.com/goodwin/shopify/manuals/#Slider)"
},
{
"type": "header",
"content": "Layout"
},
{
"type":"select",
"id":"block_width",
"label":"Width",
"options":[
{"value":"boxed","label":"Boxed"},
{"value":"fullboxed","label":"Fullboxed"},
{"value":"fullwidth","label":"Fullwidth"},
{"value":"global_width","label":"Use global site width"}
],
"default":"fullwidth",
"info":"click Save to see changes"
},
{
"type": "select",
"id": "outer_margin",
"label": "Outer top margin",
"options":[
{"value":"mt-0","label":"None"},
{"value":"mt-1","label":"10px"},
{"value":"mt-2","label":"20px"},
{"value":"mt-3","label":"30px"},
{"value":"mt-4","label":"40px"},
{"value":"mt-5","label":"50px"},
{"value":"mt-6","label":"60px"},
{"value":"mt-65","label":"65px"},
{"value":"mt-7","label":"70px"},
{"value":"mt-8","label":"80px"},
{"value":"mt-9","label":"90px"},
{"value":"mt-10","label":"100px"}
],
"default": "mt-0"
},
{
"type": "checkbox",
"id": "show_under_header",
"label": "Show block under the header",
"default": false,
"info":"usefull when column is enabled"
},
{
"type": "header",
"content": "General"
},
{
"type": "checkbox",
"id": "autoplay",
"label": "Auto-rotate slides",
"default": true
},
{
"type": "checkbox",
"id": "dots",
"label": "Show dots",
"info":"slider navigation",
"default": true
},
{
"type": "checkbox",
"id": "dotspos",
"label": "Vertical dots",
"info":"slider dots position (if 'Show dots' checked)",
"default": true
},
{
   "type": "radio",
   "id": "dotsposide",
   "options": [
      { "value": "vert-dots--right", "label": "Right"},
	  { "value": "vert-dots--left", "label": "Left"}
   ],
   "label": "Vertical Dots Position"
},
{
"type": "checkbox",
"id": "dotsfullwidth",
"label": "Dots always fullwidth",
"info":"slider dots always fullwidth",
"default": false
},
{
"type": "checkbox",
"id": "arrows",
"label": "Show arrows",
"info":"slider navigation",
"default": true
},
{
"type": "checkbox",
"id": "arowsdark",
"label": "Dark arrows",
"info":"slider arrows dark (if 'Show arrows' checked)",
"default": true
},
{
"type": "checkbox",
"id": "arowsbg",
"label": "Arrows with background",
"default": false
},
{
"type": "checkbox",
"id": "arowsshift",
"label": "Shifted arrows",
"info":"slider dots position (if 'Show dots' checked)",
"default": false
},
{
   "type": "radio",
   "id": "arowsshiftside",
   "options": [
      { "value": "bnslider-arrows--pullright", "label": "Shift arrows to the right"},
	  { "value": "bnslider-arrows--pullleft", "label": "Shift arrows to the left"}
   ],
   "label": "Shifted arrows position"
},
{
"type": "select",
"id": "autoplay_speed",
"label": "Autoplay speed",
"default": "5000",
"options": [
{ "value": "3000", "label": "3 seconds" },
{ "value": "4000", "label": "4 seconds" },
{ "value": "5000", "label": "5 seconds" },
{ "value": "7000", "label": "7 seconds" },
{ "value": "10000", "label": "10 seconds" },
{ "value": "15000", "label": "15 seconds" }
]
},
{
"type": "select",
"id": "slideshow_height",
"label": "Section height",
"default": "lg",
"options": [
{ "value": "sm", "label": "Small" },
{ "value": "md", "label": "Medium" },
{ "value": "lg", "label": "Large" },
{ "value": "fullheight", "label": "Fullheight" }
],
"info":"this option will be ignored if you check 'Keep aspect ratio'"
},
{
"type": "paragraph",
"content": "Custom slider height/width. We advise to use these settings if you want to keep original slides bg images width/height for mobile and desktop"
},
{
"type": "checkbox",
"id": "keep_scale",
"label": "Keep aspect ratio",
"info":"(it makes own proportion for the slider)",
"default": false
},
{
"type": "text",
"id": "keep_scale_width",
"label": "Slider width",
"default": "1920",
"info": "Only if 'Keep aspect ratio' is checked. We advise to write original slide image width in px"
},
{
"type": "text",
"id": "keep_scale_height",
"label": "Slider height",
"default": "820",
"info": "Only if 'Keep aspect ratio' is checked. We advise to write original slide image height in px"
},
{
"type": "paragraph",
"content": "Only For Mobile (max 768px):"
},
{
"type": "text",
"id": "keep_scale_mwidth",
"label": "Slider width",
"default": "1920"
},
{
"type": "text",
"id": "keep_scale_mheight",
"label": "Slider height",
"default": "820"
},
{
"type": "checkbox",
"id": "snow",
"label": "Add snow effect",
"default": false
},
{
"type": "header",
"content": "Side Column"
},
{
"type": "checkbox",
"id": "right_column",
"label": "Enable",
"default": false
},
{
"type":"select",
"id":"column",
"label":"Column position",
"default":"right",
"options":[
{
"value":"left",
"label":"Left"
},
{
"value":"right",
"label":"Right"
}
]
},
{
"type":"select",
"id":"bnr_source",
"label":"Ð¡reate banner with:",
"default":"admin",
"options":[
{
"value":"online",
"label":"Banner generator"
},
{
"value":"admin",
"label":"Admin panel"
}
],
"info":"*All banner's setting in the Admin panel don't apply for a banner made with the Banner generator."
},
{
"type":"textarea",
"id":"html",
"label":"HTML (from Banner generator)",
"info":"*Will apply only if you check 'Banner generator'. Generate banners [here](http://frontend.big-skins.com/goodwin-html/banners-generator)"
},
{
"type": "paragraph",
"content": "To add the banner via Admin, click '+Add Content' and select 'Banner for side column'"
}
],
"blocks": [
